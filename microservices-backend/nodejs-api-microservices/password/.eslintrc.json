{
    "extends": "airbnb",
    "rules": {
        "linebreak-style": "off",
        "indent": ["error", 4],
        "padded-blocks": ["error", "always"],
        "no-underscore-dangle": ["error", {"allow": ["_id"]}],
        "react/jsx-indent": ["error", 4]
    },
    "env": {
        "browser": true
    }
}